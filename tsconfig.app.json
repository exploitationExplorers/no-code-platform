{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*", "vite.config.ts"],
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "strict": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
